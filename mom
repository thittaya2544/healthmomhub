    <script>
        let currentContent = null;

        function enterContent(type) {
            document.getElementById('coverPage').classList.add('hidden');
            document.getElementById('mainContent').classList.remove('hidden');
            
            // Hide all content first
            document.getElementById('pregnancyContent').classList.add('hidden');
            document.getElementById('postpartumContent').classList.add('hidden');
            document.getElementById('babyContent').classList.add('hidden');
            
            // Show selected content
            if (type === 'pregnancy') {
                document.getElementById('pregnancyContent').classList.remove('hidden');
                currentContent = 'pregnancy';
            } else if (type === 'postpartum') {
                document.getElementById('postpartumContent').classList.remove('hidden');
                currentContent = 'postpartum';
            } else if (type === 'baby') {
                document.getElementById('babyContent').classList.remove('hidden');
                currentContent = 'baby';
            }
        }

        function goHome() {
            document.getElementById('mainContent').classList.add('hidden');
            document.getElementById('coverPage').classList.remove('hidden');
            currentContent = null;
        }

        // Initialize page
        document.addEventListener('DOMContentLoaded', function() {
            document.getElementById('coverPage').classList.remove('hidden');
            document.getElementById('mainContent').classList.add('hidden');
        });
    </script>
